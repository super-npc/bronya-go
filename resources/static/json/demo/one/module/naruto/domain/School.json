{
  "type": "page",
  "body":
  [
    {
      "type": "crud",
      "syncLocation": false,
      "autoGenerateFilter":
      {
        "columnsNum": 5,
        "defaultCollapsed": true,
        "showBtnToolbar": true
      },
      "defaultCollapsed": true,
      "api":
      {
        "method": "post",
        "url": "/admin/base/amis/page",
        "data":
        {
          "&": "$$",
          "One2ManyReq.entity": "${One2ManyReq.entity}",
          "One2ManyReq.entityField": "${One2ManyReq.entityField}",
          "One2ManyReq.entityFieldVal": "${One2ManyReq.entityFieldVal}",
          "BindMiddleChild.entity": "${BindMiddleChild.entity}",
          "BindMiddleChild.selfField": "${BindMiddleChild.selfField}",
          "BindMiddleChild.selfFieldVal": "${BindMiddleChild.selfFieldVal}",
          "BindMiddleChild.joinField": "${BindMiddleChild.joinField}"
        },
        "headers":
        {
          "bean": "School"
        }
      },
      "quickSaveApi":
      {
        "method": "post",
        "url": "/admin/base/amis/quick-save-api",
        "headers":
        {
          "bean": "School"
        }
      },
      "headerToolbar":
      [
        "reload",
        {
          "type": "columns-toggler",
          "align": "right",
          "draggable": true,
          "icon": "fas fa-cog",
          "footerBtnSize": "sm"
        },
        "bulkActions",
        {
          "label": "新增",
          "type": "button",
          "actionType": "dialog",
          "level": "primary",
          "dialog":
          {
            "title": "新增数据",
            "size": "lg",
            "body":
            {
              "type": "form",
              "api":
              {
                "method": "post",
                "url": "/admin/base/amis/create",
                "headers":
                {
                  "bean": "School"
                }
              },
              "body":
              [
                {
                  "label": "校名",
                  "name": "School__name",
                  "required": true,
                  "static": false,
                  "type": "input-text",
                  "value": "xx学校"
                }
              ]
            }
          }
        }
      ],
      "footerToolbar":
      [
        "statistics",
        {
          "type": "pagination",
          "showPageInput": true,
          "layout": "perPage,pager,go"
        }
      ],
      "bulkActions":
      [
        {
          "type": "hidden",
          "label": "必须有兜底的按钮,不然会出现无法多选的情况"
        },
        {
          "label": "批量删除",
          "actionType": "ajax",
          "level": "danger",
          "api":
          {
            "method": "post",
            "url": "/admin/base/amis/delete-batch",
            "data":
            {
              "ids": "$ids"
            },
            "headers":
            {
              "bean": "School"
            }
          },
          "confirmText": "确认批量删除? ids:「$ids」"
        }
      ],
      "itemActions":
      [
        {
          "actionType": "dialog",
          "dialog":
          {
            "actions":
            [
              {
                "actionType": "confirm",
                "confirmText": "确定需要'取消关联'  学校.id =「$id」",
                "label": "取消关联",
                "level": "danger",
                "type": "button"
              }
            ],
            "body":
            {
              "api":
              {
                "data":
                {
                  "middleClass": "RefSchoolStudent",
                  "thisMiddleFiled": "schoolId",
                  "thisMiddleFiledVal": "$id",
                  "anOtherMiddleField": "studentId",
                  "relationshipType": "connect",
                  "&": "$$"
                },
                "headers":
                {
                  "bean": "School"
                },
                "method": "POST",
                "url": "/admin/base/amis/middle-relationship"
              },
              "body":
              {
                "type": "picker",
                "name": "anOtherMiddleFieldVal",
                "joinValues": true,
                "required": false,
                "disabled": false,
                "valueField": "id",
                "labelField": "Student__name",
                "label": "",
                "source":
                {
                  "method": "post",
                  "url": "/admin/base/amis/page",
                  "data":
                  {
                    "BindMiddleChild.joinField": "schoolId",
                    "BindMiddleChild.selfFieldVal": "$id",
                    "&": "$$",
                    "BindMiddleChild.middleEntity": "RefSchoolStudent",
                    "BindMiddleChild.relationshipType": "connect",
                    "BindMiddleChild.selfField": "id"
                  },
                  "headers":
                  {
                    "bean": "Student"
                  }
                },
                "modalSize": "xl",
                "size": "xl",
                "embed": true,
                "multiple": true,
                "pickerSchema":
                {
                  "type": "crud",
                  "syncLocation": false,
                  "autoGenerateFilter": true,
                  "defaultCollapsed": true,
                  "draggable": true,
                  "headerToolbar":
                  {
                    
                  },
                  "footerToolbar":
                  [
                    "statistics",
                    {
                      "type": "pagination",
                      "showPageInput": true,
                      "layout": "perPage,pager,go"
                    }
                  ],
                  "columns":
                  [
                    {
                      "label": "主键",
                      "name": "Student__id",
                      "searchable":
                      {
                        "label": "主键",
                        "mode": "horizontal",
                        "name": "Student__id",
                        "placeholder": "请输入主键",
                        "type": "input-number"
                      },
                      "sort": -100,
                      "static": true,
                      "type": "static"
                    },
                    {
                      "label": "学生名字",
                      "name": "Student__name",
                      "searchable":
                      {
                        "label": "学生名字",
                        "mode": "horizontal",
                        "name": "Student__name",
                        "placeholder": "请输入学生名字",
                        "type": "input-text"
                      },
                      "static": true,
                      "type": "static"
                    },
                    {
                      "label": "年龄",
                      "name": "Student__age",
                      "searchable":
                      {
                        "label": "年龄",
                        "mode": "horizontal",
                        "name": "Student__age",
                        "placeholder": "请输入年龄",
                        "precision": -1,
                        "type": "input-number"
                      },
                      "static": true,
                      "type": "static"
                    },
                    {
                      "itemSchema":
                      {
                        "color": "${color}",
                        "displayMode": "normal",
                        "label": "${label}",
                        "type": "tag"
                      },
                      "label": "性别",
                      "labelField": "label",
                      "map":
                      [
                        {
                          "color": "#87CEEB",
                          "displayMode": "normal",
                          "label": "男",
                          "name": "BOY",
                          "type": "tag"
                        },
                        {
                          "color": "#4B0082",
                          "displayMode": "normal",
                          "label": "女",
                          "name": "GIRL",
                          "type": "tag"
                        }
                      ],
                      "name": "Student__sex",
                      "searchable":
                      {
                        "label": "性别",
                        "mode": "horizontal",
                        "name": "Student__sex",
                        "options":
                        [
                          {
                            "label": "男",
                            "value": "BOY"
                          },
                          {
                            "label": "女",
                            "value": "GIRL"
                          }
                        ],
                        "placeholder": "请输入性别",
                        "type": "select"
                      },
                      "static": true,
                      "type": "static-mapping",
                      "valueField": "name"
                    },
                    {
                      "label": "创建人",
                      "name": "Student__createBy",
                      "sort": 100,
                      "static": true,
                      "toggled": false,
                      "type": "static"
                    },
                    {
                      "label": "最后更新人",
                      "name": "Student__updateBy",
                      "sort": 101,
                      "static": true,
                      "toggled": false,
                      "type": "static"
                    },
                    {
                      "label": "创建时间",
                      "name": "Student__createTime",
                      "sort": 102,
                      "static": true,
                      "toggled": false,
                      "type": "static-datetime",
                      "width": "153px"
                    },
                    {
                      "label": "最后更新时间",
                      "name": "Student__updateTime",
                      "sort": 103,
                      "static": true,
                      "toggled": false,
                      "type": "static-datetime",
                      "width": "153px"
                    }
                  ]
                }
              },
              "type": "form"
            },
            "size": "xl",
            "title": "多对多 查询:学生, 学校.id =「$id」选择数据并进行 取消关联"
          },
          "hiddenOnHover": true,
          "label": "学生(n:n关联)",
          "type": "button"
        },
        {
          "actionType": "dialog",
          "dialog":
          {
            "actions":
            [
              {
                "actionType": "confirm",
                "confirmText": "确定需要'关联数据'  学校.id =「$id」",
                "label": "关联数据",
                "level": "warning",
                "type": "button"
              }
            ],
            "body":
            {
              "api":
              {
                "data":
                {
                  "middleClass": "RefSchoolStudent",
                  "thisMiddleFiled": "schoolId",
                  "thisMiddleFiledVal": "$id",
                  "anOtherMiddleField": "studentId",
                  "relationshipType": "disconnect",
                  "&": "$$"
                },
                "headers":
                {
                  "bean": "School"
                },
                "method": "POST",
                "url": "/admin/base/amis/middle-relationship"
              },
              "body":
              {
                "type": "picker",
                "name": "anOtherMiddleFieldVal",
                "joinValues": true,
                "required": false,
                "disabled": false,
                "valueField": "id",
                "labelField": "Student__name",
                "label": "",
                "source":
                {
                  "method": "post",
                  "url": "/admin/base/amis/page",
                  "data":
                  {
                    "BindMiddleChild.joinField": "schoolId",
                    "BindMiddleChild.selfFieldVal": "$id",
                    "&": "$$",
                    "BindMiddleChild.middleEntity": "RefSchoolStudent",
                    "BindMiddleChild.relationshipType": "disconnect",
                    "BindMiddleChild.selfField": "id"
                  },
                  "headers":
                  {
                    "bean": "Student"
                  }
                },
                "modalSize": "xl",
                "size": "xl",
                "embed": true,
                "multiple": true,
                "pickerSchema":
                {
                  "type": "crud",
                  "syncLocation": false,
                  "autoGenerateFilter": true,
                  "defaultCollapsed": true,
                  "draggable": true,
                  "headerToolbar":
                  {
                    
                  },
                  "footerToolbar":
                  [
                    "statistics",
                    {
                      "type": "pagination",
                      "showPageInput": true,
                      "layout": "perPage,pager,go"
                    }
                  ],
                  "columns":
                  [
                    {
                      "label": "主键",
                      "name": "Student__id",
                      "searchable":
                      {
                        "label": "主键",
                        "mode": "horizontal",
                        "name": "Student__id",
                        "placeholder": "请输入主键",
                        "type": "input-number"
                      },
                      "sort": -100,
                      "static": true,
                      "type": "static"
                    },
                    {
                      "label": "学生名字",
                      "name": "Student__name",
                      "searchable":
                      {
                        "label": "学生名字",
                        "mode": "horizontal",
                        "name": "Student__name",
                        "placeholder": "请输入学生名字",
                        "type": "input-text"
                      },
                      "static": true,
                      "type": "static"
                    },
                    {
                      "label": "年龄",
                      "name": "Student__age",
                      "searchable":
                      {
                        "label": "年龄",
                        "mode": "horizontal",
                        "name": "Student__age",
                        "placeholder": "请输入年龄",
                        "precision": -1,
                        "type": "input-number"
                      },
                      "static": true,
                      "type": "static"
                    },
                    {
                      "itemSchema":
                      {
                        "color": "${color}",
                        "displayMode": "normal",
                        "label": "${label}",
                        "type": "tag"
                      },
                      "label": "性别",
                      "labelField": "label",
                      "map":
                      [
                        {
                          "color": "#87CEEB",
                          "displayMode": "normal",
                          "label": "男",
                          "name": "BOY",
                          "type": "tag"
                        },
                        {
                          "color": "#4B0082",
                          "displayMode": "normal",
                          "label": "女",
                          "name": "GIRL",
                          "type": "tag"
                        }
                      ],
                      "name": "Student__sex",
                      "searchable":
                      {
                        "label": "性别",
                        "mode": "horizontal",
                        "name": "Student__sex",
                        "options":
                        [
                          {
                            "label": "男",
                            "value": "BOY"
                          },
                          {
                            "label": "女",
                            "value": "GIRL"
                          }
                        ],
                        "placeholder": "请输入性别",
                        "type": "select"
                      },
                      "static": true,
                      "type": "static-mapping",
                      "valueField": "name"
                    },
                    {
                      "label": "创建人",
                      "name": "Student__createBy",
                      "sort": 100,
                      "static": true,
                      "toggled": false,
                      "type": "static"
                    },
                    {
                      "label": "最后更新人",
                      "name": "Student__updateBy",
                      "sort": 101,
                      "static": true,
                      "toggled": false,
                      "type": "static"
                    },
                    {
                      "label": "创建时间",
                      "name": "Student__createTime",
                      "sort": 102,
                      "static": true,
                      "toggled": false,
                      "type": "static-datetime",
                      "width": "153px"
                    },
                    {
                      "label": "最后更新时间",
                      "name": "Student__updateTime",
                      "sort": 103,
                      "static": true,
                      "toggled": false,
                      "type": "static-datetime",
                      "width": "153px"
                    }
                  ]
                }
              },
              "type": "form"
            },
            "size": "xl",
            "title": "多对多 查询:学生, 学校.id =「$id」选择数据并进行 关联数据"
          },
          "hiddenOnHover": true,
          "label": "学生(n:n未关联)",
          "type": "button"
        },
        {
          "label": "详情",
          "type": "button",
          "hiddenOnHover": true,
          "actionType": "dialog",
          "dialog":
          {
            "title": "查看数据「$id」",
            "size": "lg",
            "body":
            {
              "type": "form",
              "initApi":
              {
                "method": "post",
                "url": "/admin/base/amis/view",
                "data":
                {
                  "id": "$id"
                },
                "headers":
                {
                  "bean": "School"
                }
              },
              "body":
              [
                {
                  "label": "主键",
                  "name": "School__id",
                  "required": false,
                  "static": true,
                  "type": "input-text"
                },
                {
                  "label": "校名",
                  "name": "School__name",
                  "required": true,
                  "static": true,
                  "type": "input-text"
                },
                {
                  "label": "创建人",
                  "name": "School__createBy",
                  "static": true,
                  "type": "input-number"
                },
                {
                  "label": "最后更新人",
                  "name": "School__updateBy",
                  "static": true,
                  "type": "input-number"
                },
                {
                  "label": "创建时间",
                  "name": "School__createTime",
                  "static": true,
                  "type": "input-datetime"
                },
                {
                  "label": "最后更新时间",
                  "name": "School__updateTime",
                  "static": true,
                  "type": "input-datetime"
                }
              ]
            }
          }
        },
        {
          "type": "button",
          "label": "修改",
          "hiddenOnHover": true,
          "actionType": "dialog",
          "dialog":
          {
            "title": "修改数据「$id」",
            "size": "lg",
            "body":
            {
              "type": "form",
              "initApi":
              {
                "method": "post",
                "url": "/admin/base/amis/view",
                "data":
                {
                  "id": "$id"
                },
                "headers":
                {
                  "bean": "School"
                }
              },
              "api":
              {
                "method": "post",
                "url": "/admin/base/amis/update",
                "headers":
                {
                  "bean": "School"
                }
              },
              "body":
              [
                {
                  "label": "主键",
                  "name": "School__id",
                  "required": false,
                  "static": false,
                  "type": "hidden"
                },
                {
                  "label": "校名",
                  "name": "School__name",
                  "required": true,
                  "static": false,
                  "type": "input-text",
                  "value": "xx学校"
                }
              ]
            }
          }
        },
        {
          "label": "删除",
          "type": "button",
          "hiddenOnHover": true,
          "actionType": "ajax",
          "level": "danger",
          "confirmText": "确认要删除? id:「$id」",
          "api":
          {
            "method": "post",
            "url": "/admin/base/amis/delete-batch",
            "data":
            {
              "ids": "$id"
            },
            "headers":
            {
              "bean": "School"
            }
          }
        }
      ],
      "itemBadge":
      {
        "text": "${badgeText}",
        "mode": "ribbon",
        "position": "top-left",
        "level": "${badgeLevel}",
        "visibleOn": "this.badgeText"
      },
      "rowClassNameExpr": "${badgeText == '未读' ? 'bg-green-50' : ''}",
      "columns":
      [
        {
          "label": "主键",
          "name": "School__id",
          "sort": -100,
          "static": true,
          "type": "static"
        },
        {
          "label": "校名",
          "name": "School__name",
          "static": true,
          "type": "static"
        },
        {
          "label": "创建人",
          "name": "School__createBy",
          "sort": 100,
          "static": true,
          "toggled": false,
          "type": "static"
        },
        {
          "label": "最后更新人",
          "name": "School__updateBy",
          "sort": 101,
          "static": true,
          "toggled": false,
          "type": "static"
        },
        {
          "label": "创建时间",
          "name": "School__createTime",
          "sort": 102,
          "static": true,
          "toggled": false,
          "type": "static-datetime",
          "width": "153px"
        },
        {
          "label": "最后更新时间",
          "name": "School__updateTime",
          "sort": 103,
          "static": true,
          "toggled": false,
          "type": "static-datetime",
          "width": "153px"
        }
      ]
    }
  ]
}