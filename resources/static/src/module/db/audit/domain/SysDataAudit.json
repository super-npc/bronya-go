{
  "type": "page",
  "body":
  [
    {
      "type": "crud",
      "syncLocation": false,
      "autoGenerateFilter":
      {
        "columnsNum": 5,
        "defaultCollapsed": true,
        "showBtnToolbar": true
      },
      "defaultCollapsed": true,
      "api":
      {
        "method": "post",
        "url": "/admin/base/amis/page",
        "data":
        {
          "&": "$$",
          "One2ManyReq.entity": "${One2ManyReq.entity}",
          "One2ManyReq.entityField": "${One2ManyReq.entityField}",
          "One2ManyReq.entityFieldVal": "${One2ManyReq.entityFieldVal}",
          "BindMiddleChild.entity": "${BindMiddleChild.entity}",
          "BindMiddleChild.selfField": "${BindMiddleChild.selfField}",
          "BindMiddleChild.selfFieldVal": "${BindMiddleChild.selfFieldVal}",
          "BindMiddleChild.joinField": "${BindMiddleChild.joinField}"
        },
        "headers":
        {
          "bean": "SysDataAudit"
        }
      },
      "quickSaveApi":
      {
        "method": "post",
        "url": "/admin/base/amis/quick-save-api",
        "headers":
        {
          "bean": "SysDataAudit"
        }
      },
      "headerToolbar":
      [
        "reload",
        {
          "type": "columns-toggler",
          "align": "right",
          "draggable": true,
          "icon": "fas fa-cog",
          "footerBtnSize": "sm"
        },
        "bulkActions"
      ],
      "footerToolbar":
      [
        "statistics",
        {
          "type": "pagination",
          "showPageInput": true,
          "layout": "perPage,pager,go"
        }
      ],
      "bulkActions":
      [
        {
          "type": "hidden",
          "label": "必须有兜底的按钮,不然会出现无法多选的情况"
        }
      ],
      "itemActions":
      [
        {
          "label": "详情",
          "type": "button",
          "hiddenOnHover": true,
          "actionType": "dialog",
          "dialog":
          {
            "title": "查看数据「$id」",
            "size": "lg",
            "body":
            {
              "type": "form",
              "initApi":
              {
                "method": "post",
                "url": "/admin/base/amis/view",
                "data":
                {
                  "id": "$id"
                },
                "headers":
                {
                  "bean": "SysDataAudit"
                }
              },
              "body":
              [
                {
                  "label": "主键",
                  "name": "SysDataAudit__id",
                  "required": false,
                  "static": true,
                  "type": "input-text"
                },
                {
                  "label": "表",
                  "name": "SysDataAudit__tableBean",
                  "required": true,
                  "static": true,
                  "type": "input-text"
                },
                {
                  "label": "主键",
                  "name": "SysDataAudit__recordPrimaryKey",
                  "required": true,
                  "static": true,
                  "type": "input-text"
                },
                {
                  "label": "修改前",
                  "name": "SysDataAudit__oldData",
                  "required": true,
                  "static": true,
                  "type": "editor"
                },
                {
                  "label": "修改后",
                  "name": "SysDataAudit__newData",
                  "required": true,
                  "static": true,
                  "type": "editor"
                },
                {
                  "label": "更新人",
                  "name": "SysDataAudit__updateBy",
                  "required": true,
                  "static": true,
                  "type": "input-number"
                },
                {
                  "label": "更新时间",
                  "name": "SysDataAudit__updateTime",
                  "required": true,
                  "static": true,
                  "type": "input-datetime"
                }
              ]
            }
          }
        }
      ],
      "itemBadge":
      {
        "text": "${badgeText}",
        "mode": "ribbon",
        "position": "top-left",
        "level": "${badgeLevel}",
        "visibleOn": "this.badgeText"
      },
      "rowClassNameExpr": "${badgeText == '未读' ? 'bg-green-50' : ''}",
      "columns":
      [
        {
          "label": "主键",
          "name": "SysDataAudit__id",
          "sort": -100,
          "static": false,
          "type": "static"
        },
        {
          "label": "表",
          "name": "SysDataAudit__tableBean",
          "static": true,
          "type": "static"
        },
        {
          "label": "主键",
          "name": "SysDataAudit__recordPrimaryKey",
          "static": true,
          "type": "static"
        },
        {
          "label": "更新人",
          "name": "SysDataAudit__updateBy",
          "static": true,
          "type": "static"
        },
        {
          "label": "更新时间",
          "name": "SysDataAudit__updateTime",
          "static": true,
          "type": "static-datetime",
          "width": "153px"
        },
        {
          "diffValue": "$SysDataAudit__newData",
          "label": "差异",
          "language": "json",
          "name": "SysDataAudit__oldData",
          "type": "diff-editor",
          "width": "1500px",
          "className": "text-success",
          "static": true
        }
      ]
    }
  ]
}